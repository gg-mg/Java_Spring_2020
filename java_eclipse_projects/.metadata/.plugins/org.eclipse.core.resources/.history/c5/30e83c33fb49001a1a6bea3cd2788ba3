
public class Test {
	public static void main(String[] args) {
		nPrintln(5, "welcome to Java!");
	} // end of main

	double score = 100;

	public static void nPrintln(int n, String msg) {
		for (int i = 0; i < 5; i++) {
			System.out.println(msg);
			System.out.println('a' == 'a');
			System.out.println('a' >= 'A');
			System.out.println('A' >= 'a');
			System.out.println('a' != 'b');
//			System.out.println('a' == "a");

			if (score > 90) {
				score += 1;
				System.out.println(score);
			} // end of nPrintln

		}
	} // end of class
}